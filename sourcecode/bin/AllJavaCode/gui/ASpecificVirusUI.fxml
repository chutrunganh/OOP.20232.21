<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <ImageView fx:id="homeButton" fitHeight="40.0" fitWidth="40.0" layoutX="10.0" layoutY="450.0" onMouseClicked="#homeButtonClicked" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@image/home-button.png" />
               </image>
            </ImageView>
            <TabPane layoutX="6.0" prefHeight="309.0" prefWidth="634.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Structure">
                     <content>
                        <BorderPane prefHeight="265.0" prefWidth="634.0">
                           <right>
                              <ImageView fx:id="virusImage" fitHeight="143.0" fitWidth="167.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
                                 <BorderPane.margin>
                                    <Insets right="30.0" />
                                 </BorderPane.margin>
                              </ImageView>
                           </right>
                           <center>
                              <TextArea fx:id="taVirusInfo" prefHeight="222.0" prefWidth="323.0" BorderPane.alignment="CENTER">
                                 <BorderPane.margin>
                                    <Insets left="10.0" top="5.0" />
                                 </BorderPane.margin>
                              </TextArea>
                           </center>
                        </BorderPane>
                     </content>
                </Tab>
                <Tab text="Infect Mechanism">
                     <content>
                        <AnchorPane prefHeight="318.0" prefWidth="635.0">
                           <children>
                              <TextArea fx:id="taVirusInfectMechanism" layoutX="-5.0" prefHeight="54.0" prefWidth="640.0" />
                           </children>
                        </AnchorPane>
                     </content></Tab>
              </tabs>
            </TabPane>
            <Button fx:id="helpButton" layoutX="726.0" layoutY="450.0" mnemonicParsing="false" onMouseClicked="#helpButtonClicked" prefHeight="40.0" prefWidth="64.0" text="Help">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
    </AnchorPane>
  </children>
</VBox>
