<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AllJavaCode.GUI.LoadAllVirusController">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About us" />
                        <MenuItem mnemonicParsing="false" text="How to use this app" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <ImageView fx:id="image1" fitHeight="200.0" fitWidth="200.0" layoutX="50.0" layoutY="187.0" onMouseClicked="#CoronaClicked" pickOnBounds="true" preserveRatio="true" />
                <ImageView fx:id="image2" fitHeight="200.0" fitWidth="200.0" layoutX="350.0" layoutY="187.0" onMouseClicked="#HIVClicked" pickOnBounds="true" preserveRatio="true" />
                <ImageView fx:id="image3" fitHeight="200.0" fitWidth="200.0" layoutX="650.0" layoutY="187.0" onMouseClicked="#RotaClicked" pickOnBounds="true" preserveRatio="true" />
                <Label fx:id="Corona" alignment="CENTER" contentDisplay="TEXT_ONLY" layoutX="100.0" layoutY="428.0">
                    <font>
                        <Font name="System Bold" size="17.0" />
                    </font>
                </Label>
                <Label fx:id="HIV" alignment="CENTER" contentDisplay="TEXT_ONLY" layoutX="400.0" layoutY="428.0">
                    <font>
                        <Font name="System Bold" size="17.0" />
                    </font>
                </Label>
                <Label fx:id="Rota" alignment="CENTER" contentDisplay="TEXT_ONLY" layoutX="700.0" layoutY="428.0">
                    <font>
                        <Font name="System Bold" size="17.0" />
                    </font>
                </Label>
                <ImageView fitHeight="44.0" fitWidth="45.0" layoutX="14.0" layoutY="516.0" onMouseClicked="#backToHomeClicked" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@ImageAndVideo/home-button.png" />
                    </image>
                </ImageView>
            </children>
        </AnchorPane>
    </children>
</VBox>
